    <script language="javascript" type="text/javascript">
    $(document).ready(function () {
        $.ajax({
            type: "POST",
            url: "/KMServices/WSUIHelper.asmx/GetShoppingCartItemCount",
            data: '{}',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (msg) {
                $('#spanbasket').text(msg.d + " ürün var");
            },
            error: function (xhr, ajaxOptions, thrownError) {
                $('#spanbasket').text("");
            }

        });



        $('ul.ProductsMenu').each(function () {
            var nav = $(this);
            nav
    .mouseover(function (e) {
        nav.doTimeout('li', 250, over, e.target);
    }).mouseout(function () {
        nav.doTimeout('li', 250, out);
    });

            function over(elem) {
                var parent = $(elem).closest('li');

                out(parent);
                parent.children('a').addClass('hover');
                /*parent.children( 'ul:hidden' ).slideDown( 'fast' );*/
                parent.children('ul:hidden').fadeIn('fast');
            };

            function out(elem) {
                var parents = elem
      ? $(elem).closest('li').siblings()
      : nav.children();


                parents.children('a').removeClass('hover');
                parents.children('ul').hide();
            };

        });

        /*load first sub*/
        $('ul.ProductsMenu > li > ul > li:first-child > ul').show();
        $('ul.ProductsMenu > li > ul > li:first-child > a').addClass("hover");

    });
    </script>
